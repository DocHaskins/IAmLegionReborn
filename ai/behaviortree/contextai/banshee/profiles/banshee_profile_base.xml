<?xml version="1.0" encoding="UTF-8"?>
<ConstParams xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../MetaData/schema_const.xsd">
    <!-- Jump attack -->
    <Param m_Name="$jump_attack_enabled" m_Value="true"/>
    <Param m_Name="$jump_attack_min_range" m_Value="4.0"/>
    <Param m_Name="$jump_attack_max_range" m_Value="16.0"/>
    <Param m_Name="$jump_attack_max_angle" m_Value="45.0"/>
    <Param m_Name="$jump_attack_cooldown" m_Value="1.0"/>

    <Param m_Name="$try_to_find_nearest_high_ground_position" m_Value="true"/>
    <Param m_Name="$jump_attack_from_above_enabled" m_Value="true"/>
    <Param m_Name="$jump_attack_from_above_min_range" m_Value="4.0"/>
    <Param m_Name="$jump_attack_from_above_max_range" m_Value="16.0"/>
    <Param m_Name="$jump_attack_from_above_max_angle" m_Value="45.0"/>
    <Param m_Name="$jump_attack_from_above_cooldown" m_Value="2.0"/>

    <Param m_Name="$jump_stagger_after_perfect_dodge_time" m_Value="3.0"/>
    <!-- Running attack -->
    <Param m_Name="$running_low_attack_enabled" m_Value="true"/>
    <Param m_Name="$running_low_attack_min_range" m_Value="8.0"/>
    <Param m_Name="$running_low_attack_start_range" m_Value="9.0"/>
    <Param m_Name="$running_low_attack_max_range" m_Value="17.0"/>
    <Param m_Name="$running_low_attack_approach_range" m_Value="30.0"/>
    <Param m_Name="$running_low_attack_cooldown" m_Value="1.0"/>
    <!-- Jump low attack -->
    <Param m_Name="$jump_low_attack_enabled" m_Value="true"/>
    <Param m_Name="$jump_low_attack_min_range" m_Value="8.0"/>
    <Param m_Name="$jump_low_attack_max_range" m_Value="11.0"/>
    <Param m_Name="$jump_low_attack_approach_range" m_Value="30.0"/>
    <Param m_Name="$jump_low_attack_cooldown" m_Value="1.0"/>
    <!-- Dodge consts -->
    <Param m_Name="$enable_dodge" m_Value="true"/>
    <Param m_Name="$dodge_cooldown" m_Value="1.0"/>
    <!-- Other combat params -->
    <Param m_Name="$preferred_distance_to_target_min" m_Value="9.0"/>
    <Param m_Name="$preferred_distance_to_target_max" m_Value="12.0"/>
    <Param m_Name="$max_keep_distance_range" m_Value="20.0"/>
    <Param m_Name="$jump_between_biters_enabled" m_Value="false"/>
    <Param m_Name="$max_jumps_between_biters" m_Value="0"/>
    <Param m_Name="$enable_flanking_run" m_Value="true"/>
	<Param m_Name="$trail_min_range" m_Value="40.0"/>
    <!-- strafing -->
    <Param m_Name="$strafe_enabled" m_Value="true"/>1
    <Param m_Name="$try_to_flank_when_strafing" m_Value="true"/>
    <Param m_Name="$target_prediciton_time" m_Value="0.05"/>
    <!-- combat slots -->
    <Param m_Name="$combat_slot_attack_limit" m_Value="2"/>

    <!-- dir correction speed -->
    <Param m_Name="$running_attack_dir_correction_speed" m_Value="160.0"/>
    <Param m_Name="$jump_attack_dir_correction_speed" m_Value="120.0"/>
    <Param m_Name="$hit_reaction_dir_correction_speed" m_Value="240.0"/>
    <!-- Hit Reactions -->
    <Param m_Name="$enable_hit_phase_incremental" m_Value="false"/>
    <Param m_Name="$hit_phase_incremental_reset_time" m_Value="3.0"/>
    <Param m_Name="$enable_hit_phase_use_remaining_health_only" m_Value="false"/>
    <Param m_Name="$hit_phase_min" m_Value="1"/>
    <Param m_Name="$hit_phase_max" m_Value="3"/>
    <Param m_Name="$ragdoll_death_chance" m_Value="1.0"/>
    <Param m_Name="$death_snap_angle" m_Value="90"/>
    <Param m_Name="$hit_snap_angle" m_Value="90"/>
    <!-- Misc -->
	<Param m_Name="$movestyle" m_Value="Run"/>
    <Param m_Name="$enable_weapon_change" m_Value="false"/>
    <Param m_Name="$enable_weapon_set" m_Value="false"/>
    <Param m_Name="$use_avoidance" m_Value="false"/>
    <Param m_Name="$avoid_regions_preset" m_Value="zombie"/>
    <Param m_Name="$uv_immunity" m_Value="false"/>
    <Param m_Name="$hunting_mod" m_Value="true"/>

    <!-- UV light death -->
    <Param m_Name="$min_time_in_uv_to_die" m_Value="9.0"/>
    <Param m_Name="$max_time_in_uv_to_die" m_Value="12.0"/>
    <Param m_Name="$min_time_in_sunlight_to_die" m_Value="9.0"/>
    <Param m_Name="$max_time_in_sunlight_to_die" m_Value="12.0"/>
</ConstParams>
