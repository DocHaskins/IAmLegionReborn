<?xml version="1.0" encoding="UTF-8"?>
<ConstParams xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../MetaData/schema_const.xsd">
    <ExternalParams m_UID="bolter_profile_base"/>
    <!-- Difficulty consts -->
    <Param m_Name="$push_enabled" m_Value="true"/>
    <Param m_Name="$push_cooldown" m_Value="5.0"/>

    <Param m_Name="$fart_enabled" m_Value="true"/>
    <Param m_Name="$fart_cooldown" m_Value="10.0"/>

    <Param m_Name="$super_movement_enabled" m_Value="false"/>
    <Param m_Name="$super_movement_duration" m_Value="5.0"/>
    <Param m_Name="$super_movement_cooldown" m_Value="15.0"/>   <!-- this counts from START of the super_move -->
    <Param m_Name="$super_movement_speed_multiplier" m_Value="1.0"/>
    <Param m_Name="$super_movement_preparation_time" m_Value="0.0"/>
    <Param m_Name="$super_movement_initial_cooldown" m_Value="5.0"/> <!-- time between spotting danger and activating -->

    <Param m_Name="$base_move_speed_multiplier" m_Value="1.2"/>
    <Param m_Name="$base_movestyle" m_Value="RunFast"/> <!-- Run or RunFast for now -->

    <Param m_Name="$search_rooftop_retreat_pos" m_Value="true"/>

    <Param m_Name="$bomb_charges" m_Value="3"/>

</ConstParams>
