<?xml version="1.0"?>
<ConstParams xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../MetaData/schema_const.xsd">
    <ExternalParams m_UID="human_consts_ovrd_bow"/>
    <!-- overrides - bow tier 2 -->
    <!-- defensive moves -->
    <Param m_Name="$attack_push_max_range" m_Value="3.5"/>
    <!-- bow - single shot -->
    <Param m_Name="$enable_bow_shot_single" m_Value="true"/>
    <Param m_Name="$bow_shot_single_cooldown" m_Value="1"/>
    <!-- bow - combo shot -->
    <Param m_Name="$enable_bow_shot_combo" m_Value="true"/>
    <Param m_Name="$bow_shot_combo_cooldown" m_Value="4"/>
    <Param m_Name="$bow_shot_combo_delay" m_Value="1.5"/>
    <Param m_Name="$bow_shot_combo_count" m_Value="2"/>
    <!-- bow - multi shot -->
    <Param m_Name="$enable_bow_shot_multi" m_Value="false"/>
    <!-- bow - common -->
    <Param m_Name="$bow_aim_delay" m_Value="0.4"/>
    <Param m_Name="$bow_arrow_speed" m_Value="25"/>
    <Param m_Name="$bow_arrow_correction_angle" m_Value="0"/>
    <!-- group tokens -->
    <Param m_Name="$attack_range_min_delay" m_Value="0.75"/>
    <Param m_Name="$attack_range_limit" m_Value="1"/>
    <!-- defensive moves -->
    <Param m_Name="$enable_dodge" m_Value="true"/>
</ConstParams>
