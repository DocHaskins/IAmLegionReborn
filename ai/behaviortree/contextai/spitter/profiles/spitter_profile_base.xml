<?xml version="1.0"?>
<ConstParams xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../MetaData/schema_const.xsd">

    <Param m_Name="$spit_enabled" m_Value="true"/>
    <Param m_Name="$spit_min_range" m_Value="4.5"/>
    <Param m_Name="$spit_max_range" m_Value="15.0"/>
    <Param m_Name="$spit_max_angle" m_Value="60"/>

    <Param m_Name="$quick_spit_enabled" m_Value="false"/>
    <Param m_Name="$quick_spit_max_range" m_Value="6.0"/>
    <Param m_Name="$quick_spit_max_angle" m_Value="45"/>
    <Param m_Name="$quick_spit_cooldown" m_Value="3"/>

    <Param m_Name="$spit_combo_cooldown" m_Value="6"/>
    <Param m_Name="$spit_combo_attacks_min" m_Value="2"/>
    <Param m_Name="$spit_combo_attacks_max" m_Value="4"/>

    <Param m_Name="$mortar_spit_enabled" m_Value="false"/>
    <Param m_Name="$mortar_spit_min_range" m_Value="10.0"/>
    <Param m_Name="$mortar_spit_max_range" m_Value="20.0"/>
    <Param m_Name="$mortar_spit_cooldown" m_Value="15"/>
    <Param m_Name="$mortar_combo_attacks_min" m_Value="1"/>
    <Param m_Name="$mortar_combo_attacks_max" m_Value="1"/>
    <Param m_Name="$mortar_spit_cancel_range" m_Value="6"/>
    <Param m_Name="$mortar_spit_prediction_multiplier" m_Value="0.025"/>

    <Param m_Name="$push_enabled" m_Value="false"/>
    <Param m_Name="$push_max_range" m_Value="2.5"/>
    <Param m_Name="$push_max_angle" m_Value="60"/>
    <Param m_Name="$push_cooldown" m_Value="12"/>

    <Param m_Name="$defensive_vomit_enabled" m_Value="false"/>
    <Param m_Name="$defensive_vomit_cooldown" m_Value="15"/>

    <Param m_Name="$projectile_dodge_enabled" m_Value="false"/>
    <Param m_Name="$projectile_dodge_cooldown" m_Value="10"/>

    <Param m_Name="$startle_cooldown" m_Value="15"/>

    <!-- dir correction speed -->
    <Param m_Name="$push_dir_correction_speed" m_Value="90.0"/>
    <Param m_Name="$spit_dir_correction_speed" m_Value="300.0"/>
    <Param m_Name="$quick_spit_dir_correction_speed" m_Value="600.0"/>
    <Param m_Name="$flashlight_dodge_dir_correction_speed" m_Value="240.0"/>
    <!-- other consts -->
    <Param m_Name="$target_prediciton_time" m_Value="0.25"/>
    <!-- sensor presets -->

    <Param m_Name="$death_snap_angle" m_Value="90"/>
    <Param m_Name="$ragdoll_death_chance" m_Value="1.0"/>

    <Param m_Name="$hit_snap_angle" m_Value="90"/>
    <Param m_Name="$hit_phase_min" m_Value="0"/>
    <Param m_Name="$hit_phase_max" m_Value="4"/>
    <Param m_Name="$hit_phase_incremental_reset_time" m_Value="6"/>
    <Param m_Name="$enable_hit_phase_incremental" m_Value="false"/>
    <Param m_Name="$enable_hit_phase_use_remaining_health_only" m_Value="false"/>

    <!-- weapon -->
    <Param m_Name="$enable_weapon_change" m_Value="false"/>
    <Param m_Name="$enable_weapon_set" m_Value="false"/>
    <Param m_Name="$enable_debug_hold" m_Value="false"/>

    <Param m_Name="$use_avoidance" m_Value="false"/>
    <Param m_Name="$avoid_regions_preset" m_Value="zombie"/>
    <Param m_Name="$hunting_mod" m_Value="true"/>

    <!-- UV light death -->
    <Param m_Name="$min_time_in_uv_to_die" m_Value="9.0"/>
    <Param m_Name="$max_time_in_uv_to_die" m_Value="12.0"/>
    <Param m_Name="$min_time_in_sunlight_to_die" m_Value="9.0"/>
    <Param m_Name="$max_time_in_sunlight_to_die" m_Value="12.0"/>

    <Param m_Name="$disable_parkour_ragdoll" m_Value="false"/>
</ConstParams>
