        AIRagdollHitDamagePreset("domino_hit_heavy@ai_damage_presets.scr");
        AIDominoHitDamagePreset("human_domino_hit@ai_damage_presets.scr");
		
		AIMovePushReactionPreset("gentle_push@damage_reaction_presets.scr");
