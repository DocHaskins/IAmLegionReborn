import "ial_settings.mth"

sub DefaultParams()
{
    FallHeightImmediateKill("15.0");

    WallHitMinRelativeHeight("0.9");
    WallHitMinCollisionDepth("0.2");
    WallHitMinSelfSpeed("2.0");

    AIDominoHitMinSpeed("2.0");

    FallDamageTag("fall");
    WallHitDamageTag("wall_hit");
    SpikesStaticDamageTag("spikes");
    SpikesDynamicDamageTag("spikes_dynamic");
}

